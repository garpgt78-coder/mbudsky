loadstring(game:HttpGet("https://raw.githubusercontent.com/dibbot/mobile-log/main/main.lua"))()

-- Atau paste langsung:

local MobileSniffer = {
    Enabled = true,
    LogRemote = true,
    LogChat = true,
    LogPlayer = true,
    LogDamage = true,
    LogPickup = true,
    LogKill = true,
    MaxLogs = 50, -- Lebih dikit biar ga lag di HP
    AutoScroll = true
}

