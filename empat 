local LogList = Instance.new("ScrollingFrame")
LogList.Size = UDim2.new(1, -10, 1, -10)
LogList.Position = UDim2.new(0, 5, 0, 5)
LogList.BackgroundTransparency = 1
LogList.ScrollBarThickness = 6
LogList.CanvasSize = UDim2.new(0, 0, 0, 0)
LogList.Parent = LogFrame

local LogLayout = Instance.new("UIListLayout")
LogLayout.Padding = UDim.new(0, 4)
LogLayout.Parent = LogList

-- Log storage
local logs = {}
local logCount = 0

-- Helper functions
local function getTime()
    return os.date("%H:%M:%S")
end

local function addLog(category, message, color)
    if not MobileSniffer.Enabled then return end
    
    -- Filter
    if category == "REMOTE" and not MobileSniffer.LogRemote then return end
    if category == "CHAT" and not MobileSniffer.LogChat then return end
    if category == "PLAYER" and not MobileSniffer.LogPlayer then return end
    if category == "DAMAGE" and not MobileSniffer.LogDamage then return end
    if category == "PICKUP" and not MobileSniffer.LogPickup then return end
    if category == "KILL" and not MobileSniffer.LogKill then return end
    
    -- Log entry (font lebih gede buat mobile)
    local logEntry = Instance.new("Frame")
    logEntry.Size = UDim2.new(1, 0, 0, 40)
    logEntry.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    logEntry.BorderSizePixel = 0
    
    local entryCorner = Instance.new("UICorner")
    entryCorner.CornerRadius = UDim.new(0, 6)
    entryCorner.Parent = logEntry
    
    local timeLabel = Instance.new("TextLabel")
    timeLabel.Size = UDim2.new(0, 60, 1, 0)
    timeLabel.BackgroundTransparency = 1
    timeLabel.Text = getTime()
    timeLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
    timeLabel.Font = Enum.Font.Code
    timeLabel.TextSize = 14
    timeLabel.TextXAlignment = Enum.TextXAlignment.Center
    timeLabel.Parent = logEntry
    
    local catLabel = Instance.new("TextLabel")
    catLabel.Size = UDim2.new(0, 70, 1, 0)
    catLabel.Position = UDim2.new(0, 60, 0, 0)
    catLabel.BackgroundTransparency = 1
    catLabel.Text = "[" .. category .. "]"
    catLabel.TextColor3 = color or Color3.fromRGB(100, 200, 255)
    catLabel.Font = Enum.Font.Code
    catLabel.TextSize = 14
    catLabel.TextXAlignment = Enum.TextXAlignment.Center
    catLabel.Parent = logEntry
    
    local msgLabel = Instance.new("TextLabel")
    msgLabel.Size = UDim2.new(1, -140, 1, 0)
    msgLabel.Position = UDim2.new(0, 130, 0, 0)
    msgLabel.BackgroundTransparency = 1
    msgLabel.Text = message
    msgLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    msgLabel.Font = Enum.Font.Code
    msgLabel.TextSize = 14
    msgLabel.TextXAlignment = Enum.TextXAlignment.Left
    msgLabel.TextWrapped = true
    msgLabel.Parent = logEntry
    
    logEntry.Parent = LogList
    
    table.insert(logs, logEntry)
    logCount = logCount + 1
    
    if logCount > MobileSniffer.MaxLogs then
        local oldest = table.remove(logs, 1)
        if oldest then oldest:Destroy() end
        logCount = logCount - 1
    end
    
    if MobileSniffer.AutoScroll then
        task.wait()
        LogList.CanvasPosition = Vector2.new(0, LogList.CanvasSize.Y.Offset)
    end
end

ClearBtn.MouseButton1Click:Connect(function()
    for _, log in ipairs(logs) do
        log:Destroy()
    end
    table.clear(logs)
    logCount = 0
end)

-- ===== SNIFFER FUNCTIONS (sama seperti versi PC) =====
-- (Copy dari script sebelumnya, disingkat biar ga terlalu panjang)

-- Remote sniffer
local function hookRemote(remote)
    if remote:IsA("RemoteEvent") then
        local old = remote.FireServer
        remote.FireServer = function(s, ...)
            local args = {...}
            local str = ""
            for i=1, math.min(2, #args) do
                str = str .. tostring(args[i]) .. (i < #args and ", " or "")
            end
            if #args > 2 then str = str .. "..." end
            addLog("REMOTE", remote.Name .. "(" .. str .. ")", Color3.fromRGB(255, 150, 0))
            return old(s, ...)
        end
    end
end

for _, v in ipairs(ReplicatedStorage:GetDescendants()) do hookRemote(v) end
ReplicatedStorage.DescendantAdded:Connect(function(v) task.wait(0.1); hookRemote(v) end)

-- Chat sniffer
for _, p in ipairs(Players:GetPlayers()) do
    p.Chatted:Connect(function(m) addLog("CHAT", p.Name .. ": " .. m, Color3.fromRGB(0, 255, 100)) end)
end
Players.PlayerAdded:Connect(function(p)
    p.Chatted:Connect(function(m) addLog("CHAT", p.Name .. ": " .. m, Color3.fromRGB(0, 255, 100)) end)
end)

-- Player join/leave
Players.PlayerAdded:Connect(function(p) addLog("PLAYER", p.Name .. " joined", Color3.fromRGB(100, 200, 255)) end)
Players.PlayerRemoving:Connect(function(p) addLog("PLAYER", p.Name .. " left", Color3.fromRGB(255, 100, 100)) end)

-- Damage & kill
local function onChar(char)
    local hum = char:WaitForChild("Humanoid")
    hum.HealthChanged:Connect(function()
        if hum.Health < (hum.Health + 10) then
            addLog("DAMAGE", char.Name .. " took damage", Color3.fromRGB(255, 50, 50))
        end
    end)
    hum.Died:Connect(function() addLog("KILL", char.Name .. " died", Color3.fromRGB(255, 0, 255)) end)
end

for _, p in ipairs(Players:GetPlayers()) do
    if p.Character then onChar(p.Character) end
    p.CharacterAdded:Connect(onChar)
end

-- Pickup
workspace.DescendantAdded:Connect(function(obj)
    if obj:IsA("BasePart") and (obj.Name:lower():find("item") or obj.Name:lower():find("loot")) then
        obj.Touched:Connect(function(hit)
            if hit.Parent == LocalPlayer.Character then
                addLog("PICKUP", "Picked up " .. obj.Name, Color3.fromRGB(255, 255, 0))
            end
        end)
    end
end)

-- Output
game:GetService("LogService").MessageOut:Connect(function(msg, t)
    if t == Enum.MessageType.MessageOutput then
        addLog("OUTPUT", msg, Color3.fromRGB(150, 150, 150))
    elseif t == Enum.MessageType.MessageError then
        addLog("ERROR", msg, Color3.fromRGB(255, 0, 0))
    end
end)

